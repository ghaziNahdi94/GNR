{
    "name": "FB Friend suggestions blocker",
    "description": "Block all Facebook friend suggestions !",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
      "16": "16.png",
      "48": "48.png",
      "128": "128.png"
    },
    "content_scripts": [
        {
          "matches": ["https://www.facebook.com/*"],
          "js": ["script.js"]
        }
      ]
  }